<h1>Show all islands a user have visited</h1>

<h2>
	<%= @user.name %>
	<%= link_to('DELETE', user_path(@user.id), method: :delete)  %>
</h2>

<ul>
	<% @user.islands.each do |i| %>
		<li>
			<%= i[:name] %>
		</li>
	<% end %>
</ul>

<h2>Islands you haven't visited</h2>
<ul>
	<% (Island.all.map(&:name) - @user.islands.map(&:name)).each do |island| %>
		<li><%= island  %></li>
	<% end %>
</ul>
